{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "operator-linebreak": "off",
        "arrow-body-style": "off",
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0,
        "react/function-component-definition": 0
    }
}
